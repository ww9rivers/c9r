{
"Version":	"$Id: csvfix-conf.json,v 1.15 2015/12/04 14:49:43 weiwang Exp $",
"":
[	"Run 'csvfix.py -h' for configuration details.",

	"A configuration for parsing Prime Infrastructure device list generated",
	"by the ~/oss/netopy/cisco/pi/nssdevices.py script, which is in JSON",
	"format.",
	""
],

	"path":	"/app/nss/var/pi/devices",
	"tasks":
	{
		"":	"Radiology Devices",
		"json-to-csv":
		{
			"disabled":	false,
			"pattern":	"nssdevices-????-??-??.json",
			"delete":	false,
			"input-format":	"json",
			"rename":	{ "^(.+).json$": "{0}.csv" },
			"file-mode":	"w",
			"header":	[
				"collectionTime",
				"ipAddress",
				"deviceName",
				"deviceType",
				"softwareType",
				"softwareVersion",
				"loc",
				"location",
				"reachability",
				"managementStatus"
			],
			"write-header":	true
		},
		

		"":	" - End of Removed Task List - "
	},

	"dialects":
	{
		"nix":
		{
			"quoting":		"QUOTE_ALL",
			"lineterminator":	"\n"
		}
	},
"":	"--- End ---"
}
